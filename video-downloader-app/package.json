{
  "name": "video-downloader-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:clean": "npm run clean:cache && vite",
    "dev:fresh": "npm run clean:full && npm install && vite",
    "predev": "npm run validate-cache",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint",
    "validate-jsx": "node scripts/validate-jsx.js",
    "validate-imports": "node scripts/validate-imports.cjs",
    "validate-cache": "node scripts/validate-vite-cache.cjs",
    "validate-duplicates": "node scripts/validate-duplicates.js",
    "validate-paths": "node scripts/validate-paths.js",
    "check-modules": "node scripts/check-modules.js",
    "validate-all": "npm run validate-imports && npm run check-modules && npm run validate-duplicates && npm run validate-paths",
    "clean": "rm -rf node_modules/.vite dist",
    "clean:full": "rm -rf node_modules node_modules/.vite dist package-lock.json",
    "clean:cache": "rm -rf node_modules/.vite",
    "fix-vite": "node scripts/fix-vite-errors.cjs",
    "pre-build-check": "npm run clean && npm run validate-all",
    "preview": "vite preview",
    "server": "node server.cjs",
    "postinstall": "npm run validate-cache",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ],
    "src/**/*.{ts,tsx}": [
      "npm run type-check -- --noEmit"
    ]
  },
  "dependencies": {
    "@distube/ytdl-core": "^4.16.12",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "@tailwindcss/vite": "^4.1.17",
    "@types/react-window": "^1.8.8",
    "axios": "^1.13.2",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dexie": "^4.2.1",
    "dexie-react-hooks": "^4.2.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "framer-motion": "^12.23.24",
    "jszip": "^3.10.1",
    "lucide-react": "^0.555.0",
    "postcss": "^8.5.6",
    "puppeteer": "^24.31.0",
    "qrcode.react": "^4.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.6",
    "react-window": "^2.2.3",
    "recharts": "^3.5.1",
    "tailwindcss": "^4.1.17",
    "youtubei.js": "^16.0.1",
    "ytdl-core": "^4.11.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}
